{
    "name": "pixxio/akeneo-pixxio-connector",
    "description": "Pixx.io integration to Akeneo 5+ CE",
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Pixxio\\PixxioConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pixxio\\PixxioConnector\\Test\\": "tests/"
        }
    },
    "conflict": {
        "akeneo/pim-enterprise-dev": "*"
    },
    "require": {
        "akeneo/pim-community-dev": "^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.1",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "overtrue/phplint": "^3.0",
        "symfony/debug-bundle": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "archive": {
        "exclude": ["/spec/", "/tests/", "/.github/", ".php_cs.php", "phpunit.xml.dist", "/.images/", "/doc/"]
    }
}
